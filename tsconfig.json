{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "rootDir": "./src",
    "outDir": "./out",
    "jsx": "react",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "sourceMap": false
  },
    "exclude": [
      ".vscode",
      "node_modules",
      "out"
    ],
  "files": [
    "src/actions.ts",
    "src/index.ts",
    "src/Settings.tsx"
  ]
}